{
  "name": "spacecat",
  "displayName": "Space Cat Theme",
  "version": "0.0.16",
  "description": "A theme from outer space",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "Theme",
    "Dark",
    "Spacecat",
    "Modern",
    "vivid",
    "Node.js",
    "cat",
    "space",
    "spacecat"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/franzgollhammer/spacecat"
  },
  "license": "MIT",
  "author": {
    "name": "Franz Gollhammer"
  },
  "publisher": "franzgollhammer",
  "scripts": {
    "package": "npx @vscode/vsce package",
    "publish": "npx @vscode/vsce publish",
    "release": "npm run upgrade-package-version && npm run sort-package && npm run package && npm run publish",
    "sort-package": "npx sort-package-json ",
    "upgrade-package-version": "node ./scripts/upgrade-package-version.js"
  },
  "contributes": {
    "themes": [
      {
        "label": "Spacecat",
        "uiTheme": "vs-dark",
        "path": "./themes/Spacecat-color-theme.json"
      },
      {
        "label": "Spacecat Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Spacecat Dark-color-theme.json"
      },
      {
        "label": "Spacecat Black",
        "uiTheme": "vs-dark",
        "path": "./themes/Spacecat Black-color-theme.json"
      },
      {
        "label": "Spacecat Solarized",
        "uiTheme": "vs-dark",
        "path": "./themes/Spacecat Solarized-color-theme.json"
      }
    ]
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "icon": "icon.png"
}